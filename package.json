{
	"name": "delta",
	"version": "0.0.0",
	"description": "Wings of Fire Moderation Discord Bot",
	"main": "dist/index.js",
	"type": "module",
	"repository": "https://github.com/wofwiki/Delta",
	"engines": {
		"node": "18.16.0",
		"pnpm": "8.3.1"
	},
	"scripts": {
		"start": "node --experimental-specifier-resolution=node --enable-source-maps .",
		"dev": "nodemon",
		"build": "pnpm swc && pnpm type-check",
		"type-check": "tsc --noEmit",
		"swc": "swc src --copy-files --config-file \".swcrc\" -d \"dist\"",
		"pretty": "prettier --check \"src/**/*.ts\"",
		"pretty-fix": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint \"src/**/*.ts\"",
		"lint-fix": "eslint --fix \"src/**/*.ts\"",
		"prisma": "prisma generate"
	},
	"author": "wofwiki <wings.of.fire.fandom.wiki@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"@prisma/client": "4.13.0",
		"discord.js": "14.9.0",
		"dotenv": "16.0.3",
		"fero-dc": "4.0.0-dev-21",
		"fero-ms": "2.0.7",
		"glob": "10.2.2",
		"lodash": "4.17.21"
	},
	"devDependencies": {
		"@swc/cli": "0.1.62",
		"@swc/core": "1.3.56",
		"@types/glob": "8.1.0",
		"@types/lodash": "4.14.194",
		"@types/node": "18.16.3",
		"@typescript-eslint/eslint-plugin": "5.59.1",
		"@typescript-eslint/parser": "5.59.1",
		"eslint": "8.39.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-prettier": "4.2.1",
		"nodemon": "2.0.22",
		"prettier": "2.8.8",
		"prisma": "4.13.0",
		"ts-node": "10.9.1",
		"typescript": "5.0.4"
	},
	"packageManager": "pnpm@8.3.1"
}
