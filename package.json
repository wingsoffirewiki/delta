{
  "name": "delta",
  "version": "0.0.0",
  "description": "Wings of Fire Moderation Discord Bot",
  "main": "dist/src/index.js",
  "repository": "https://github.com/wofwiki/Delta",
  "engines": {
    "node": "18.14.0",
    "npm": "9.3.1"
  },
  "scripts": {
    "run": "node --enable-source-maps .",
    "start": "yarn build && yarn run run",
    "dev": "nodemon",
    "build": "tsc",
    "pretty": "prettier --check \"src/**/*.ts\"",
    "pretty-fix": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint-fix": "eslint --fix \"src/**/*.ts\"",
    "prisma": "prisma generate"
  },
  "author": "wofwiki <wings.of.fire.fandom.wiki@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "4.0.0",
    "axios": "0.25.0",
    "discord.js": "13.6.0",
    "dotenv": "14.2.0",
    "fero-dc": "4.0.0-dev-6",
    "fero-ms": "2.0.7",
    "glob": "7.2.0",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@types/glob": "7.2.0",
    "@types/lodash": "4.14.178",
    "@types/node": "17.0.13",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "nodemon": "2.0.15",
    "prettier": "2.6.2",
    "prisma": "4.0.0",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  }
}
